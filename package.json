{
  "name": "angular-play",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Material, TypeScript, and ReactiveX.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "rimraf public && webpack-dev-server --progress",
    "build": "rimraf public functions && webpack --env.prod",
    "lint": "tslint src/client.ts 'src/**/*.ts' && stylelint 'src/**/*.css'",
    "unit": "cross-env JEST_ENV=app jest \"src/.*\\.ts$\" --coverage --forceExit",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.ts$\" --forceExit",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular-Play.git"
  },
