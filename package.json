{
  "name": "AngularNest-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Angular, Nest, Material, Express, TypeScript, and ReactiveX.",
  "scripts": {
    "start:app": "rimraf public && webpack-dev-server --progress",
    "start:api": "concurrently \"tsc -w -p tsconfig.api.json\" \"nodemon functions/server.js\"",
    "build:app": "rimraf public && webpack --env.prod",
    "build:api": "tsc -p tsconfig.api.json",
    "lint:app": "echo \"TODO\"",
    "lint:api": "echo \"TODO\"",
    "test:app": "cross-env TEST_ENV=app jest \"src/app/.*\\.ts$\" --coverage --forceExit",
    "test:api": "cross-env TEST_ENV=api jest \"src/api/.*\\.ts$\" --coverage --forceExit",
    "e2e:app": "echo \"TODO\"",
    "e2e:api": "echo \"TODO\"",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/AngularNest-Fullstack-Starter.git"
  },
  "keywords": [
    "Angular",
    "Nest",
    "Fullstack"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
